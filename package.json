    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@segment/analytics-node": "^1.0.0-beta.26",
    "@sendgrid/mail": "^7.7.0",
    "@toruslabs/eccrypto": "^2.1.1",
    "alchemy-sdk": "^2.6.2",
    "aws-sdk": "^2.1348.0",
    "axios": "^1.3.4",
